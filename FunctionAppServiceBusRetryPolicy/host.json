{
    "version": "2.0",
    "logging": {
        "logLevel": {
            "default": "Information",
            "Host": "Error",
            "Function": "Error",
            "Host.Aggregator": "Information"
        },
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        }
    },
    "extensions": {
        "autoCompleteMessages": false,
        "maxAutoLockRenewalDuration": "00:06:00",
        "maxConcurrentCalls": 1,
        "maxConcurrentSessions": 1
    }
}